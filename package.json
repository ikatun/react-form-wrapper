{
  "name": "react-form-wrapper",
  "version": "0.0.2",
  "description": "Compose controlled forms in React.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rhythnic/react-form-wrapper.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register 'test/**/*.@(js)'",
    "test:watch": "npm run test -- --watch",
    "compile": "babel -d dist/ src/",
    "start": "webpack-dev-server --inline --content-base compiled-examples --history-api-fallback",
    "build:dev": "webpack"
  },
  "keywords": [
    "React",
    "Forms",
    "Serialize",
    "Controlled forms"
  ],
  "authors": [
    "Nick Baroni <nick@rhythnic.com> (https://github.com/rhythnic)"
  ],
  "bugs": {
    "url": "https://github.com/rhythnic/react-form-wrapper/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "expect": "^1.14.0",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "react-dom": "^0.14.7"
  },
  "peerDependencies": {
    "react": "^0.14.7",
    "immutable": "^3.7.6",
    "react-addons-pure-render-mixin": "^0.14.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
